<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical"
    android:scrollbars="vertical" >
    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Intervals" />

    <EditText
        android:id="@+id/intervals"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="4,8,32,64,300"
        android:ems="10" >
    </EditText>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="First Fix Timeout" />

    <EditText
        android:id="@+id/first_fix_timeout"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:ems="10" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="High Speed Threshold"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <EditText
        android:id="@+id/high_speed_threshold"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:ems="10" />
    
    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Horizontal Acc. Threshold"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <EditText
        android:id="@+id/hor_acc_threshold"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:ems="10" />
    
    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Low Speed Threshold"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <EditText
        android:id="@+id/low_speed_threshold"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:ems="10" />
    
    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Back Off Timer Hor Acc"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <EditText
        android:id="@+id/backoff_timer_hor_acc"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:ems="10" />
    
    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Back Off Timer Time Threshold"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <EditText
        android:id="@+id/backoff_timer_time_threshold"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:ems="10" />
    
    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Back Off Timer Possible Move"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <EditText
        android:id="@+id/backoff_timer_poss_move"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:ems="10" />
    
    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Back Off Timer Probable Move"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <EditText
        android:id="@+id/backoff_timer_prob_move"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:ems="10" />
    
    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Back Off Timer Certain Move"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <EditText
        android:id="@+id/backoff_timer_cert_move"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:ems="10" />

    <Button
        android:id="@+id/submit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:text="Submit" />

</LinearLayout>